    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.1",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "ethers": "^5.7.1",
    "hardhat": "^2.11.2",
    "prettier": "^2.7.1",
