{
  "name": "@fluxwallet/contracts",
  "version": "0.0.1",
  "license": "MIT",
  "private": true,
  "scripts": {
    "format": "prettier '**/*.sol' --ignore-path .gitignore --write",
    "test": "hardhat test",
    "dev": "run-p dev:*",
    "dev:node": "hardhat node",
    "dev:deploy": "wait-on http://localhost:8545 && hardhat run scripts/deploy.ts --network localhost",
    "build": "hardhat compile",
    "compile:contracts": " hardhat compile --force",
    "deploy:localhost": " hardhat deploy --tags complete",
    "test:full": " run compile:circuits &&  run version:fix &&  hardhat test",
    "compile:circuits": "bash scripts/compile-circuits.sh",
    "version:fix": "node scripts/bump-solidity",
    "setup:circom": "bash scripts/setup-circom.sh"
  },
  "devDependencies": {
